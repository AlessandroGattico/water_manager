{
	"ConnectionStrings": {
		"DefaultConnection": "DataSource=app.db;Cache=Shared"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"System": "Error"
			}
		}
	},
	"AllowedHosts": "*"
}
